  },
  "bin": "lib/bin/contrib-locs.js",
  "files": [
    "README.md",
    "css/",
    "dist/",
    "lib-commonjs/",
    "lib/",
    "scss/"
  ],
  "scripts": {
    "test": "eslint && tsc --noEmit && jest",
    "start": "node lib/index.js",
    "clean": "del-cli lib lib-commonjs dist css coverage && jest --clearCache",
