    "@typescript-eslint/parser": "^3.10.0",
    "@xornot/run-script": "^0.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.3.0",
    "babel-plugin-const-enum": "^1.0.1",
    "cross-env": "^7.0.2",
    "del-cli": "^3.0.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
